#include<stdio.h>
//циклы
//предусловие
//for (инициализация до цикла; условие; что делать после цикла) {тело цикла}
//сделай что-т пока условие истина - 10
//while (условие){тело цикла}
//сделай что-то пока условие истина - 10
//постоянное условие
//do {тело цикла} while (условие)
//сделай, потом проверь
//если условие всегда истина - бесконечный цикл
//выход ctrl+c
int arrays [SIZE] = {0};
//обьявил массив arrays, размером SIZE,
//и заполнил нулями = {0}
//обход по массиву через цикл
for (int i=0; i < SIZE; ++i);
//обьявили переменную i со значением 0-счетчик(для индексов art))
{//условие пока i < SIZE(от 0 до SIZE - 1)
//инкрементирует(прибавляет +1 к i) в конце цикла
art[i]=2;
printf("arrays[%d] = %d\n", i, art[i]);
//вывод индексов элементов массива и их значений
//цикл заканчивается, выполняется ++i
}//конец блока (тела) for
//Сначало проверяется условие, потом выполняется блок(тело)
//Вновь проверка условия, если истина, то выполняется блок(пока условие не ложь)
//тоже самое, только через while
int chetchik = 0;
while(chetchik<SIZE){
        arrays[chetchik]=2
        printf("arrays[%d]= %d\n", chetchik, arrays[chetchik]);
        chetchik;
}
//если chetchik обьявить внутри while
//после каждой итерации он бы вновь равная 0
chetchik=0; //обнуляем
int sum=0; //обьявляем и присваиваем значение 0
while(chetchik < SIZE){
    sum + = chetchik; //0+1+2+3+4=10
    ++chetchik;
}





